{
  "name": "ReactTemplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open",
    "build": "NODE_ENV=production webpack",
    "build:github": "npm run build && git add ./docs*",
    "test": "NODE_ENV=test jest",
    "lint": "eslint . --ext .js --ext .jsx || exit 0",
    "lint:fix": "eslint --fix . --ext .js --ext .jsx || exit 0",
    "lint:precommit": "eslint . --ext .js --ext .jsx",
    "precommit": "npm run build:github && npm run lint:precommit",
    "prepush": "npm run lint:precommit && npm run test"
  },
  "jest": {
    "testEnvironment": "node",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.js"
    ],
    "testPathIgnorePatterns": [
      "setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sukritchhabra/ReactTemplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sukritchhabra/ReactTemplate/issues"
  },
  "homepage": "https://github.com/sukritchhabra/ReactTemplate#readme",
  "dependencies": {
    "dotenv-webpack": "^1.5.5",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "material-ui": "^1.0.0-beta.44",
    "prop-types": "^15.6.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "reduxsauce": "^1.0.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.3",
    "whatwg-fetch": "^2.0.4",
    "react-hot-loader": "^3.0.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-webpack-alias": "^2.1.2",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-hot-loader": "^1.4.3",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^1.0.0-rc.8",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.1",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.19.0",
